/**
 * @description: 主价格函数
 * @author: ccarlos
 * @date 2019/11/22 23:07
*/
function mainPrice(price,discountPrice) {
    if(!discountPrice){
        return price
    }
    else {
        return discountPrice
    }
}

/**
 * @description: 划线价格函数
 * @author: ccarlos
 * @date 2019/11/22 23:08
*/
function slashedPrice(price,discountPrice) {
    if(discountPrice){
        return price
    }else {
        return
    }
}

module.exports = {
    mainPrice:mainPrice,
    slashedPrice:slashedPrice
}